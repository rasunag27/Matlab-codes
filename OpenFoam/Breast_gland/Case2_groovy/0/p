FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    Bottom
    {
        type            calculated;
        value           $internalField;
    }

    circumference
    {
        type            calculated;
        value           $internalField;
    }

    left
    {
        type            symmetryPlane;
    }

    ahead
    {
	type		empty;
    }

    behind
    {
        type            empty;
    }
}

// ************************************************************************* //

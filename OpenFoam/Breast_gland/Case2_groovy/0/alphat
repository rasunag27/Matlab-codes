FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Bottom
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    circumference
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    left
    {
        type            symmetryPlane;
    }
    behind
    {
        type            empty;
    }
    ahead
    {
	type		empty;;
    }
}



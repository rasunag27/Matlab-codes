FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    Bottom
    {
        type            fixedValue;
	value		uniform 310;
    }
    circumference
    {
        type            fixedValue;
	value		uniform 300;
    }
    left
    {
        type            symmetryPlane;
    }
    ahead
    {
        type            empty;
    }
    behind
    {
        type            empty;
    }
}


FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-3; //0.01;

boundaryField
{
    Bottom
    {
        type            epsilonWallFunction;
        value           uniform 1e-3; //0.01;
    }
    circumference
    {
        type            epsilonWallFunction;
        value           uniform 1e-3; //0.01;
    }
    left
    {
	type		symmetryPlane;
    }
    ahead
    {
	type		empty;
    }
    behind
    {
	type		empty;
    }
}


// ************************************************************************* //

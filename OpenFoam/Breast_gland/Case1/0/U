FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Bottom
    {
        type            fixedValue;
	value		uniform (0 0 0);
    }

    circumference
    {
        type            fixedValue;
	value		uniform (0 0 0);
    }

    left
    {
	type		symmetryPlane;
    }

    ahead
    {
	type		empty;
    }

    behind
    {
	type		empty;
    }
}


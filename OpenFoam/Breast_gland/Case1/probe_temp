set title "T value at different points"
set xlabel "Iterations"
set ylabel "Temparature"

set terminal png
set output 'temparature.png'

plot "postProcessing/temparature/0/T" using 1:2 title 'point 1 at (0 0.042 0)', \
     "postProcessing/temparature/0/T" using 1:5 title 'point 2 at (0 0.072 0)'

pause 5

